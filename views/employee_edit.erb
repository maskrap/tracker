<h1>Update <%=@employee.division_id() %> </h1>

<form action="/employees/<%= @employee.id() %>" method="post">
  <input name="_method" type="hidden" value="patch">
  <label for="division_id">Reassign your employee to a different division:</label>
  <input id="division_id" name="division_id" type="text">

  <button type="submit">Update</button>
</form>
